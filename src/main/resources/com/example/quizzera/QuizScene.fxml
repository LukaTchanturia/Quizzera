<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="850.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.quizzera.QuizScene">
   <children>
      <VBox alignment="CENTER" layoutX="311.0" layoutY="182.0" prefHeight="200.0" prefWidth="100.0" spacing="25.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="quizTitleLabel" text="Quiz Title">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
            <Label text="Eeach quiz consists of 10 questions!" />
            <Label text="Once you start the quiz, you can not exit it! " />
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#backButtonClicked" prefHeight="25.0" prefWidth="68.0" text="Back" />
                  <Button mnemonicParsing="false" onAction="#buttonClicked" text="Start Quiz" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
